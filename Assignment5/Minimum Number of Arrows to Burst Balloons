class Solution {
    public int findMinArrowShots(int[][] points) {
        Arrays.sort(points,(a,b)->Integer.compare(a[0],b[0]));
        int end = points[0][1];
        int arrows = points.length;
        for(int i = 1 ; i  < points.length ; i++){
            if(points[i][0]<=end){
                arrows--;
                end = Math.min(points[i][1], end);
            }
            else{
                end = points[i][1];
            }
        }
        return arrows;
    }
}
